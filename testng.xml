<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SeleniumFW Suite" parallel="tests" thread-count="5">
    <listeners>
        <listener class-name="org.comp.Listeners"></listener>
    </listeners>
    <test name="E2E_Tests">
        <classes>
            <class name="org.test.E2E_Test"/>
        </classes>
    </test>

    <test name="ErrorValidation">
       <classes>
           <class name="org.test.ErrorValidation"/>
       </classes>
   </test>
</suite>